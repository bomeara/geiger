\name{tip.disparity}
\alias{tip.disparity}
\title{ Tip disparity calculation }
\description{
Calculates disparity for every clade in a tree
}
\usage{
tip.disparity(phy, data, disp = "avg.sq")
}
\arguments{
  \item{phy}{ Phylogenetic tree }
  \item{data}{ Morphological data }
  \item{disp}{ Disparity metric }
}
\details{
}
\value{
 	Vector of disparities
}
\references{ Harmon et al. 2003 }
\author{ Luke J. Harmon }
\note{ 
}
\seealso{ }
\examples{

data(geospiza)
attach(geospiza)

tip.disparity(geospiza.tree, geospiza.data)

}

\keyword{ arith }
